{
    "name": "drupal/acquia_search",
    "description": "Provides integration between your Drupal site and Acquia's hosted search service.",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "acquia/http-hmac-php": ">=5.0",
        "drupal/acquia_connector": "^4.0",
        "drupal/search_api_solr": "^4.3.1",
        "guzzlehttp/guzzle": "^6.5.2 || ^7.0",
        "http-interop/http-factory-guzzle": "^1.0",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "drush/drush": ">=10"
    },
    "conflict": {
        "drupal/search_api_solr_multilingual": "<3"
    },
    "extra": {
        "branch-alias": {
            "dev-3.1.x": "3.1.x-dev"
        },
        "drush": {
            "services": {
                "drush.services.yml": ">=10"
            }
        }
    }
}
