        "drupal/views_layout": "^1.2",
        "drush/drush": "^10.3",
        "npm-asset/imagesloaded": "^4.1",
        "npm-asset/masonry-layout": "^4.2",
        "oomphinc/composer-installers-extender": "^2.0",
        "spatie/pdf-to-image": "^2.0",
        "woocommerce/flexslider": "~2.0"
    },
    "require-dev": {
        "drupal/config_devel": "^1.7",
        "drupal/core-dev": "^9",
        "drupal/custom_sql_migrate_source_plugin": "^1.1",
        "drupal/devel": "^4.1",
        "drupal/migrate_devel": "^2",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.0",
        "drupal/twig_xdebug": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "file-mapping": {
              "[web-root]/.htaccess": false,
              "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-types": [
          "npm-asset"
        ],
        "installer-paths": {
          "docroot/core": ["type:drupal-core"],
          "docroot/libraries/{$name}": [
              "type:drupal-library",
              "type:npm-asset"
          ],
          "docroot/modules/contrib/{$name}": ["type:drupal-module"],
          "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
          "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
          "drupal/address": {
            "Migrate using country names": "https://www.drupal.org/files/issues/2020-02-21/country_lookup_migrate_process_plugin-3114991-2.patch"
          },
          "drupal/radix": {
            "Language switcher patch": "https://www.drupal.org/files/issues/2020-01-05/3060563-7.patch"
          },
          "drupal/media_thumbnails": {
            "Make thumbnails editable": "https://www.drupal.org/files/issues/2020-08-06/media_thumbnails-3149842-4.patch"
          },
          "drupal/core": {
            "Multiple usages of FieldPluginBase::getEntity do not check for NULL, leading to WSOD": "https://www.drupal.org/files/issues/2020-09-03/3007424-108.patch",
            "Wrong language handling in MediaThumbnailFormatter when linking": "https://www.drupal.org/files/issues/2021-05-06/2961404-17.patch",
            "Media Thumbnail Formatter doesn't translate the title attribute correctly":"https://www.drupal.org/files/issues/2019-11-12/media-thumbnail-formatter-title-not-translated-workaround-3089216-4.patch",
            "The language of content entities incorrectly falls back to the default language...":"https://www.drupal.org/files/issues/2021-09-16/3061761-46.patch"
          },
          "drupal/facets": {
            "Add option to show current search string in facet summary": "https://www.drupal.org/files/issues/2022-07-12/facets-current_search_string-2979223-55.patch",
            "Robustify how ajax finds the facet block": "https://www.drupal.org/files/issues/2022-04-12/facets-ajax-improvements-3013795-36.patch"
          },
          "drupal/ds": {
            "Provide entity context to DS block fields":"https://www.drupal.org/files/issues/2020-10-12/ds-content_entity-2939322-14.patch"
          },
          "drupal/views_field_view": {
            "Contextual filter parameter is not working":"https://www.drupal.org/files/issues/2020-09-17/multible_Contextual_filter_parameter-2873359-25.patch"
          },
          "drupal/readonlymode": {
            "Got fatal error when running this module":"https://www.drupal.org/files/issues/2020-12-23/readonlymode-string_not_array-3115119-9.patch"
          },
          "drupal/taxonomy_import": {
            "CSVs not detected properly":"https://www.drupal.org/files/issues/2021-12-17/3255009-csvs_not_detected-2.patch"
          },
          "drupal/responsive_menu": {
            "Add 'front' options to menu position":"https://www.drupal.org/files/issues/2022-06-20/responsive_menu-menuposition-3274889-3.diff"
          },
          "spatie/pdf-to-image": {
            "3221786": "https://www.drupal.org/files/issues/2021-12-03/remote-pdf-overlap-3221786-8.patch"
          },
          "drupal/views_autocomplete_filters": {
            "2999501": "https://www.drupal.org/files/issues/2022-03-22/views_autocomplete_filter-2999501-9-stable-dont_break_ajax_with_php_notice.patch"
          },
          "drupal/content_translation_redirect": {
            "3263317": "https://www.drupal.org/files/issues/2022-02-09/content-translation-redirect-all-bundles-3263317-3.patch"
          }
        }
    }
}
