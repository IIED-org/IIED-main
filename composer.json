{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": {
      "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
      "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.org"
      },
      "ckwordcount": {
      "type": "package",
      "package": {
          "name": "w8tcha/ckeditor-wordcount-plugin",
          "version": "1.17.8",
          "type": "drupal-library",
          "dist": {
              "url": "https://github.com/w8tcha/CKEditor-WordCount-Plugin/releases/download/v1.17.8/CKEditor-WordCount-Plugin.zip",
              "type": "zip"
          },
          "require": {
              "composer/installers": "~1.0"
          }
        }
      },
      "flexslider": {
        "type": "package",
        "package": {
          "name": "woocommerce/flexslider",
          "version": "2.7.2",
          "type": "drupal-library",
          "source": {
            "url": "https://github.com/woocommerce/FlexSlider.git",
            "type": "git",
            "reference": "2.7.2"
          }
        }
      }
    },
